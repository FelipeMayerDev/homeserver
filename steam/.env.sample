# Steam profiles to watch (comma separated)
PROFILES=example_profile1,example_profile2

# Steam Web API key (get from https://steamcommunity.com/dev/apikey)
STEAM_API_KEY=your_steam_api_key_here

# Check interval when user is playing (in seconds)
ACTIVE_CHECK_INTERVAL=30

# Check interval when user is offline (in seconds)
OFFLINE_CHECK_INTERVAL=300